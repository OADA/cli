{
  "name": "@oada/cli",
  "version": "3.0.0",
  "description": "CLI OADA client",
  "bin": {
    "oada": "bin/run.js"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "author": "Alex Layton <alex@layton.in>",
  "homepage": "https://github.com/OADA/cli",
  "repository": "OADA/cli",
  "bugs": "https://github.com/OADA/cli/issues",
  "license": "MIT",
  "keywords": [
    "OADA",
    "CLI",
    "oclif",
    "JSON",
    "NDJSON",
    "Concatenated-JSON",
    "LDJSON",
    "JSONL",
    "JSONLines"
  ],
  "pkg": {
    "options": {
      "enable-source-maps": true
    },
    "assets": [
      "oclif.manifest.json",
      "dist/**/*.map"
    ],
    "scripts": "dist/**/*.js",
    "targets": [
      "node16-linux-x64",
      "node16-macos-x64",
      "node16-win-x64"
    ],
    "outputPath": "packaged"
  },
  "oclif": {
    "commands": "./dist/commands",
    "topics": {
      "fs": {
        "description": "interact with OADA like a filesystem"
      }
    },
    "hooks": {
      "postrun": "./dist/hooks/postrun/close"
    },
    "bin": "oada",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-not-found",
      "@oclif/plugin-plugins"
    ]
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepare": "npm run build && oclif manifest",
    "readme": "oclif readme",
    "package": "npm run prepare && pkg .",
    "build": "tsc -b"
  },
  "files": [
    "bin",
    "dist",
    "oclif.manifest.json",
    "yarn.lock"
  ],
  "dependencies": {
    "@oada/client": "^4.1.1",
    "@oada/oadaify": "^2.1.0",
    "@oclif/command": "^1.8.16",
    "@oclif/config": "1.18.3",
    "@oclif/errors": "^1.3.5",
    "@oclif/help": "^1.0.1",
    "@oclif/plugin-autocomplete": "^1.3.0",
    "@oclif/plugin-help": "^5.1.12",
    "@oclif/plugin-not-found": "^2.3.1",
    "@oclif/plugin-plugins": "^2.1.0",
    "cli-highlight": "^2.1.11",
    "concatjson": "^2.0.1",
    "dotenv": "^16.0.1",
    "find-up": "^5.0.0",
    "gaxios": "^5.0.0",
    "hjson": "^3.2.2",
    "json-6": "^1.1.4",
    "ksuid": "^3.0.0",
    "minimatch": "^5.1.0",
    "ndjson": "^2.0.0",
    "object-assign-deep": "^0.4.0",
    "ts-node": "^10.8.0",
    "tslib": "^2.4.0",
    "type-fest": "^2.13.0"
  },
  "devDependencies": {
    "@oclif/parser": "^3.8.7",
    "@tsconfig/node16": "^1.0.2",
    "@types/glob": "^7.2.0",
    "@types/json5": "^2.2.0",
    "@types/minimatch": "^3.0.5",
    "@types/ndjson": "^2.0.1",
    "@types/node": "^16.11.38",
    "@types/object-assign-deep": "^0.4.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@yarnpkg/sdks": "^3.0.0-rc.6",
    "eslint": "^8.16.0",
    "eslint-config-xo": "^0.41.0",
    "eslint-config-xo-typescript": "^0.51.1",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-ava": "^13.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-i18n-text": "^1.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-notice": "^0.9.10",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-regexp": "^1.7.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "glob": "^8.0.3",
    "globby": "^11.1.0",
    "oclif": "^3.0.1",
    "pkg": "^5.7.0",
    "prettier": "^2.6.2",
    "typescript": "^4.8.0-dev.20220601"
  },
  "packageManager": "yarn@3.2.1"
}
